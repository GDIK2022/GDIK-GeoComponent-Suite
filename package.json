{
  "name": "@GDIK2022/GDIK-GeoComponent-Suite",
  "version": "1.0.1",
  "description": "Web Component for geometry input and visualization",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GDIK2022/GDIK-GeoComponent-Suite.git"
  },
  "scripts": {
    "test": "jest --config ./jest.config.js --collectCoverage --collectCoverageFrom=src/**/*.js",
    "test:watch": "jest --config ./jest.config.js --watch",
    "lint": "eslint ./src/** ./test/**",
    "audit": "npm audit --prod",
    "examples": "parcel example/index.html",
    "build": "parcel build"
  },
  "source": "src/index.js",
  "cdn": "dist/gdik-geocomponent-suite.js",
  "targets": {
    "cdn": {
      "includeNodeModules": true,
      "optimize": true,
      "outputFormat": "global"
    }
  },
  "author": "LGLN - Team WaaS",
  "license": "EUPL-1.2",
  "dependencies": {
    "masterportalAPI": "bitbucket:geowerkstatt-hamburg/masterportalAPI#v2.15.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@parcel/transformer-inline-string": "^2.6.2",
    "babel-jest": "^28.1.1",
    "buffer": "^6.0.3",
    "eslint": "^8.18.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "events": "^3.3.0",
    "jest": "^28.1.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^28.1.1",
    "jest-fetch-mock": "^3.0.3",
    "jest-transform-stub": "^2.0.0",
    "parcel": "^2.2.0",
    "process": "^0.11.10",
    "resize-observer-polyfill": "^1.5.1",
    "stream-browserify": "^3.0.0",
    "string_decoder": "^1.3.0",
    "timers-browserify": "^2.0.12"
  }
}
